{
  "name": "trakify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng deploy",
    "lint": "ng lint",
    "lint:fix": "yarn lint --fix",
    "lint:prettier": "npx --no-install prettier --config .prettierrc.js --list-different \"**/*.{html,scss,json,yml,js,ts}\"",
    "lint:prettier:fix": "npx --no-install prettier --config .prettierrc.js \"**/*.{html,scss,json,yml,js,ts}\" --write",
    "lint:all": "yarn lint:prettier && yarn lint",
    "fix": "yarn lint:prettier:fix && yarn lint:fix",
    "e2e": "ng e2e",
    "cypress:run": "cypress run",
    "postinstall": "patch-package"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "^12.0.2",
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/fire": "7.5.0",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@angular/service-worker": "^15.0.0",
    "angular-cli-ghpages": "1.0.3",
    "angular-oauth2-oidc": "^15.0.1",
    "date-fns": "^2.29.3",
    "ng-generic-pipe": "^15.0.1",
    "ngx-auth-firebaseui": "^7.1.0",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "rxjs": "~7.8.0",
    "sass-rem": "^3.0.0",
    "static-path": "^0.0.4",
    "tslib": "^2.4.0",
    "zod": "^3.20.2",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.0",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "^15.0.0",
    "@cypress/schematic": "2.4.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.49.0",
    "cypress": "^12.3.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.0",
    "typescript": "~4.9.4"
