{
  "name": "trakify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=Chrome",
    "test:ci": "ng test --browsers=ChromeHeadlessCI --watch=false",
    "deploy": "ng deploy",
    "lint": "ng lint --fix",
    "lint:check": "ng lint",
    "format": "prettier . --write",
    "format:check": "prettier . --list-different",
    "fix": "yarn format && yarn lint",
    "e2e": "ng e2e",
    "cypress:run": "cypress run",
    "gen:import-graph": "npx madge src\\main.ts --ts-config tsconfig.app.json --image ./docs/import-graph.svg --layout dot"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.2",
    "@angular/common": "^20.2.4",
    "@angular/compiler": "^20.2.4",
    "@angular/core": "^20.2.4",
    "@angular/fire": "20.0.1",
    "@angular/forms": "^20.2.4",
    "@angular/material": "^20.2.2",
    "@angular/platform-browser": "^20.2.4",
    "@angular/router": "^20.2.4",
    "@angular/service-worker": "^20.2.4",
    "@tanstack/angular-query-experimental": "^5.87.1",
    "angular-oauth2-oidc": "^20.0.2",
    "date-fns": "^4.1.0",
    "ngxtension": "5.1.0",
    "photoswipe": "^5.4.4",
    "rxjs": "~7.8.2",
    "static-path": "^0.0.4",
    "tslib": "^2.8.1",
    "zod": "^4.1.5",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.4",
    "@cypress/schematic": "4.1.2",
    "@types/jasmine": "~5.1.9",
    "angular-eslint": "^20.2.0",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0"
  }
}
