{
  "name": "trakify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng deploy",
    "lint": "ng lint",
    "lint:fix": "yarn lint --fix",
    "lint:prettier": "npx --no-install prettier --config .prettierrc.js --list-different \"**/*.{html,scss,json,yml,js,ts}\"",
    "lint:prettier:fix": "npx --no-install prettier --config .prettierrc.js \"**/*.{html,scss,json,yml,js,ts}\" --write",
    "lint:all": "yarn lint:prettier && yarn lint",
    "fix": "yarn lint:prettier:fix && yarn lint:fix",
    "e2e": "ng e2e",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.5",
    "@angular/cdk": "^16.1.5",
    "@angular/common": "^16.1.5",
    "@angular/compiler": "^16.1.5",
    "@angular/core": "^16.1.5",
    "@angular/fire": "7.6.1",
    "@angular/forms": "^16.1.5",
    "@angular/material": "^16.1.5",
    "@angular/platform-browser": "^16.1.5",
    "@angular/platform-browser-dynamic": "^16.1.5",
    "@angular/router": "^16.1.5",
    "@angular/service-worker": "^16.1.5",
    "angular-oauth2-oidc": "^15.0.1",
    "date-fns": "^2.29.3",
    "photoswipe": "^5.4.1",
    "rxjs": "~7.8.0",
    "sass-rem": "^3.0.0",
    "static-path": "^0.0.4",
    "tslib": "^2.6.0",
    "zod": "^3.22.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.1.5",
    "@cypress/schematic": "2.5.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "cypress": "^13.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6"
  }
}
